<%= render "shared/errors", obj: [:admin,@item] %>
<table class="ite">
  <tr>
    <th width="150">商品名</th>
    <td><%= @item.item_name %></td>
  </tr>
  <tr>
    <th>値段</th>
    <td><%= @item.price %><%= "円" %></td>
  </tr>
  <tr>
    <th>個数</th>
    <td><%= @item.item_quantity %><%= "個" %></td>
  </tr>
  <tr>
    <th>在庫</th>
    <td><%= @item.disable? ? "○" : "－" %></td>
  </tr>
  <tr>
    <th>ジャンル</th>
    <td><%= @item.author_category&.category_name %></td>
  </tr>
  </table>